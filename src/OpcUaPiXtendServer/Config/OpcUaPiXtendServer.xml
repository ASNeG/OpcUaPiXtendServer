<?xml version="1.0" encoding="utf-8"?>
<OpcUaPiXtendServer Name="TestTags" xmlns="http://ASNeG/OpcUaPiXtendServer.xsd">

    <Modules>
        <Module Enable="ON">
            <Name>PiXtendV2L</Name>
            <Type>V2L</Type>
            
            <UnitConv><Node>AO_AO1</Node><A>0</A><B>1.0</B><C>1.0</C><D>0</D></UnitConv>
            
        </Module>
        
        <Module Enable="OFF">
            <Name>PiXtendV2S</Name>
            <Type>V2S</Type>
        </Module>
        
        <Module Enable="OFF">
            <Name>DIO-1</Name>
            <Type>DO</Type>
            <Address>0x01</Address>
        </Module>
        
        <Module Enable="OFF">
          <Name>DIO-2</Name>
          <Type>DO</Type>
          <Address>0x02</Address>
        </Module>
        
        <Module Enable="OFF">
          <Name>AIO-1</Name>
          <Type>AO</Type>
          <Address>0x03</Address>
	</Module>
	
    </Modules>

</OpcUaPiXtendServer>
