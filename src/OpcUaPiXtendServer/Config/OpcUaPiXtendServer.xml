<?xml version="1.0" encoding="utf-8"?>
<OpcUaPiXtendServer Name="TestTags" xmlns="http://ASNeG/OpcUaPiXtendServer.xsd">

    <Modules>
        <Module Enable="ON">
            <Name>PiXtendV2L</Name>
            <Type>V2L</Type>
            <Unit>
                <Node>DO_DO0_Variable</Node>
                <Pin>DO0</Pin>
            </Unit>
            <Unit>
                <Node>AO_AO1_Variable</Node>
                <Pin>AO1</Pin>
                <A>0</A>
                <B>1.1</B>
                <C>1.3</C>
                <D>0</D>
            </Unit>
        </Module>
        
        <Module Enable="OFF">
            <Name>PiXtendV2S</Name>
            <Type>V2S</Type>
        </Module>
        
        <Module Enable="OFF">
            <Name>DIO-1</Name>
            <Type>DO</Type>
            <Address>0x01</Address>
        </Module>
        
        <Module Enable="OFF">
          <Name>DIO-2</Name>
          <Type>DO</Type>
          <Address>0x02</Address>
        </Module>
        
        <Module Enable="OFF">
          <Name>AIO-1</Name>
          <Type>AO</Type>
          <Address>0x01</Address>
	</Module>
	
    </Modules>

</OpcUaPiXtendServer>
